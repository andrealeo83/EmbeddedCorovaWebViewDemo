<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jimmy.embeddedcordovawebviewdemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestCordovaActivity"
            android:label="@string/title_activity_cordova" >
        </activity>
        <activity
            android:name=".TestCordovaWithLayoutActivity"
            android:label="@string/title_activity_test_cordova_with_layout" >
        </activity>
    </application>

</manifest>
